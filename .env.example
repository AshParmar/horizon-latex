# Copy this file to .env and fill in your actual API keys and IDs

# Composio API Key - Get from https://app.composio.dev/settings
COMPOSIO_API_KEY=your_composio_api_key_here

# Gmail Configuration (get from Composio Dashboard after connecting Gmail)
GMAIL_USER_ID=your_gmail_user_id_here
GMAIL_ACCOUNT_ID=your_gmail_account_id_here
GMAIL_AUTH_CONFIG_ID=your_gmail_auth_config_id_here

# LinkedIn Configuration (get from Composio Dashboard after connecting LinkedIn)
LINKEDIN_CONNECTED_ACCOUNT_ID=your_linkedin_connected_account_id_here
LINKEDIN_ENTITY_ID=your_linkedin_entity_id_here
COMPOSIO_LINKEDIN_AUTH=your_linkedin_auth_token_here

# Google Sheets Configuration (get from Composio Dashboard after connecting Google Sheets)
GOOGLE_SHEETS_AUTH_CONFIG_ID=your_google_sheets_auth_config_id_here
GOOGLE_SHEETS_ACCOUNT_ID=your_google_sheets_account_id_here
GOOGLE_SHEETS_USER_ID=your_google_sheets_user_id_here

# Groq API Key (for LLM enrichment) - Get from https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# OpenAI API Key (backup - optional) - Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here